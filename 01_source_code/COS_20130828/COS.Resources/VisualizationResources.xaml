<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:conv="clr-namespace:COS.Resources">

    <!--dark blue-->
    <Color x:Key="CosColor1">
        #1F497D
    </Color>

    <!--black-->
    <Color x:Key="CosColor2">
        #000000
    </Color>

    <!--silver-->
    <Color x:Key="CosColor3">
        #9D9D9D
    </Color>

    <!--white-->
    <Color x:Key="CosColor4">
        #FFFFFF
    </Color>

    <!--orange-->
    <Color x:Key="CosColor5">
        #FFA725
    </Color>

    <!--WhiteSmoke-->
    <Color x:Key="CosColor6">
        #e6e6e6
    </Color>

    <!--DarkGrey-->
    <Color x:Key="CosColor7">
        #767676
    </Color>

    <!--LightBlue-->
    <Color x:Key="CosColor8">
        #19b8e0
    </Color>

    <!--LightGrey-->
    <Color x:Key="CosColor9">
        #f9f9f9
    </Color>

  

    <!--fontSegoeUi-->
    <FontFamily x:Key="CosFont1">
        Segoe Ui
    </FontFamily>


    <Color x:Key="VizColor">

    </Color>

    <Color x:Key="VizColor2">

    </Color>

    <Color x:Key="VizColor3">

    </Color>

    <LinearGradientBrush x:Key="VizBorder1Brush"  StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="{StaticResource CosColor1}" Offset="0"></GradientStop>
        <GradientStop Color="{StaticResource CosColor4}" Offset="1" ></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderHeaderBrush">
        <GradientStop Color="{StaticResource CosColor1}" Offset="0"></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderSilverBrush"  StartPoint="0.5,0" EndPoint="0.5,1">
        <GradientStop Color="{StaticResource CosColor3}" Offset="0"></GradientStop>
        <GradientStop Color="{StaticResource CosColor4}" Offset="1" ></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderTargetBrush">
        <GradientStop Color="{StaticResource CosColor6}" Offset="0"></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderWgBrush">
        <GradientStop Color="{StaticResource CosColor7}" Offset="0"></GradientStop>
    </LinearGradientBrush>
    
    <LinearGradientBrush x:Key="VizBorderImageWhiteBrush">
        <GradientStop Color="{StaticResource CosColor4}" Offset="0"></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderWhiteBrush">
        <GradientStop Color="{StaticResource CosColor9}" Offset="0"></GradientStop>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="VizBorderLightBlueBrush">
        <GradientStop Color="{StaticResource CosColor8}" Offset="0"></GradientStop>
    </LinearGradientBrush>
    
    


    <Style x:Key="VizBorder1" TargetType="Border" >
        <Setter Property="Background" Value="{StaticResource VizBorder1Brush}"></Setter>
        <Setter Property="BorderThickness" Value="3"></Setter>
        <Setter Property="Height" Value="auto"></Setter>
        <Setter Property="Width" Value="auto"></Setter>
